{
  "name": "awesome-js",
  "version": "1.0.0",
  "description": "util functions and regxs for javascript",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "mocha",
    "cover": "./node_modules/.bin/istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "release": "rimraf ./dist && npm run build && npm publish"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/index.js",
  "engine": "node >= 8.x",
  "dependencies": {
    "big.js": "^5.2.2"
  },
  "devDependencies": {
    "@types/big.js": "^4.0.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "coveralls": "^3.0.6",
    "fastscan": "^1.0.6",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "rimraf": "^2.6.2",
    "should": "^13.2.3",
    "ts-node": "^8.3.0",
    "typescript": "^2.9.2"
  },
  "homepage": "https://github.com/linxiaowu66/awesome-js.git",
  "bugs": {
    "url": "https://github.com/linxiaowu66/awesome-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:linxiaowu66/awesome-js.git"
  },
